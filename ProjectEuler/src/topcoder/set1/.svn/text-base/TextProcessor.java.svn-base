package topcoder.set1;

import java.util.Arrays;

public class TextProcessor {
    public String collectLetters(String text){
        String s = text.toLowerCase();
        char[] c = s.toCharArray();
        Arrays.sort(c);
        String ret = "";
        for(char a : c)  if(Character.isLetter(a)) ret += a;
        
        
        return ret;
    }
}
