package cattledrive;


public class J2_EvenOrOdd {

    public static void main(String[] args) {
        try {
            int input = Integer.valueOf(args[0]);
            if (input % 2 == 0) {
                System.out.println("even");
            } else {
                System.out.println("odd");
            }
        } catch (Exception e) {
            System.out.println("Input must be an integer");
        }
    }
}
